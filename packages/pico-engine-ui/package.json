    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "sass": "^1.45.2",
    "scriptsp": "^1.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
