{
  "name": "pico-engine-ui",
  "private": true,
  "type": "module",
  "version": "1.2.0",
  "description": "React UI for pico-engine",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Picolab/pico-engine.git"
  },
  "author": "Picolab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Picolab/pico-engine/issues"
  },
  "homepage": "https://github.com/Picolab/pico-engine#readme",
  "scripts": {
    "start": "npm run dev -s",
    "dev": "scriptsp -r dev:tsc dev:web",
